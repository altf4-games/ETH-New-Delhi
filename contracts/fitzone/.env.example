# Deployment Environment Variables

# Private key for contract deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URLs (optional, will use public endpoints if not provided)
ARBITRUM_SEPOLIA_RPC=https://sepolia-rollup.arbitrum.io/rpc
SEPOLIA_RPC=https://rpc.sepolia.org

# API keys for contract verification (optional)
ETHERSCAN_API_KEY=your_etherscan_api_key
ARBISCAN_API_KEY=your_arbiscan_api_key

# Set to true to automatically verify contracts after deployment
VERIFY_CONTRACTS=false

# PYUSD token contract addresses
# Ethereum Sepolia - Replace with actual PYUSD address when available
PYUSD_SEPOLIA=0x9A7b2c3c853b6c1D2A8F3eF3F3F6b6c8FB2e4A7D

# Arbitrum Sepolia - Replace with actual PYUSD address when available
PYUSD_ARBITRUM_SEPOLIA=0x9A7b2c3c853b6c1D2A8F3eF3F3F6b6c8FB2e4A7D